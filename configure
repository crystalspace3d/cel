  cs_install_prefix="$ac_default_prefix"
else
  cs_install_prefix=`echo "$prefix" | sed -e 's:///*:/:g'`
fi
if test $exec_prefix = NONE; then :
  cs_install_exec_prefix="\$(prefix)"
else
