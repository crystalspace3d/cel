SubDir TOP docs ;

InstallDoc history.txt ;

SubInclude TOP docs doxygen ;
SubInclude TOP docs texinfo ;

CSDOCFILES_HTML = .html .jpg .jpeg .png .gif .css ;

SubDir TOP docs html ;
Recurse InstallDoc : $(CSDOCFILES_HTML) : html ;

