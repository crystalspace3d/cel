        <action name="AddString" >
          <par name="value" string="green" />
        </action>
        <action name="AddString" >
          <par name="value" string="blue" />
        </action>
        <action name="AddString" >
          <par name="value" string="yellow" />
        </action>
        <action name="RemoveString" >
          <par name="value" string="green" />
        </action>
